(jbuild_version 1)

(library
 ((name        interval_base)
  (public_name interval_base)
  (c_names    (interval_base_stubs))
  (c_flags    (:standard (:include c_flags.sexp)))
  (synopsis "Interval arithmetic library (base)")))

(rule
 ((targets (c_flags.sexp))
  (deps    (../config/discover.exe))
  (action  (run ${<}))))
